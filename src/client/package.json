{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "gsap": "^2.0.2",
    "howler": "^2.0.15",
    "moment": "^2.24.0",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "react-scripts": "2.1.1",
    "socket.io-client": "^2.1.1",
    "styled-components": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local": "REACT_APP_SERVER_ENDPOINT=http://localhost:3001 npm run start",
