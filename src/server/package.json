{
  "name": "func-cnb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "npm run build && node ../../lib/server/server.js",
    "serve:stat-test": "npm run build && node ../../lib/server/stats/test.js",
    "serve:local": "DEBUG=CNB:* PORT=3002 npm run serve",
    "start": "npm run build && node ../../lib/server/index.js",
    "build:deps": "cp ./package.json ../../lib/server/package.json && cd ../../lib/server && npm install --production",
    "babel": "babel ./src -d ../../lib/server --copy-files --ignore '**/*.test.js' --extensions '.ts,.js'",
    "build": "npm run babel && npm run build:deps",
    "build:test": "babel ./src -d ./test --copy-files --extensions '.ts,.js'",
    "flow": "flow ./src",
    "test": "npm run build:test && ./node_modules/.bin/mocha ./test/**/*.test.{js,ts}",
    "test:mob": "npm run build:test && ./node_modules/.bin/mocha ./test/uplift/sockets/mob-mode/**/*.test.{js,ts}",
    "lint": "eslint src",
    "verify": "npm run lint && npm run flow && npm run test",
    "check-types": "tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.4",
    "@types/mocha": "^5.2.7",
    "@types/shortid": "0.0.29",
    "@types/socket.io": "^2.1.2",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "eslint": "^5.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "flow-bin": "^0.77.0",
    "mocha": "^5.2.0",
    "sinon": "^6.3.3",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@mobily/ts-belt": "^3.11.0",
    "async": "^2.6.2",
    "aws-sdk": "^2.814.0",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "lodash": "^4.17.19",
    "moment": "^2.25.3",
    "nanoid": "^3.1.20",
    "ramda": "^0.25.0",
    "shortid": "^2.2.14",
    "socket.io": "^2.1.1",
    "supports-color": "^7.0.0",
    "uuid": "^3.3.2"
  }
