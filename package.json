  "scripts": {
    "install:server": "cd ./src/server && npm install",
    "install:client": "cd ./src/client && npm install",
    "install:client-v2": "cd ./src/client-v2 && npm install",
    "install": "npm run install:server && npm run install:client",
    "build:server": "cd ./src/server && npm run build",
    "build:client": "cd ./src/client && npm run build",
    "build:client-v2": "cd ./src/client-v2 && npm run build",
    "build": "rm -rf lib && npm run install && npm run build:server && npm run build:client && npm run build:client-v2",
    "build:docker": "rm -rf lib && npm run build:server && npm run build:client && npm run build:client-v2",
    "start": "npm run build && npm run startup",
    "startup": "node ./lib/server/hudson-server.js",
    "local:client": "cd ./src/client && npm run start:local",
    "local:server": "cd ./src/server && npm run serve:local",
    "test:client": "cd ./src/client && npm run test",
    "lint:server": "cd ./src/server && npm run lint"
